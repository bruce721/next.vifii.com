@ENABLE_GTK_DOC_TRUE@	rm -rf $(srcdir)/html 
@ENABLE_GTK_DOC_TRUE@	mkdir $(srcdir)/html
@ENABLE_GTK_DOC_TRUE@	cd $(srcdir)/html && gtkdoc-mkhtml $(DOC_MODULE) ../$(DOC_MAIN_SGML_FILE)
