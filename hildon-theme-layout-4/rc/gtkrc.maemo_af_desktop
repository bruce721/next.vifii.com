      function        = ARROW
      state           = INSENSITIVE
      arrow_direction = UP 
      overlay_file    = "../images/qgn_plat_task_switcher_menu_up_arrow_disabled.png"
      overlay_stretch = FALSE
    }
    image
    {
      function        = ARROW
      state           = SELECTED 
      arrow_direction = UP 
